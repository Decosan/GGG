<h1>Users index</h1>
<ul class="media-list">
  <% @users.each do |user| %>
	<li class="media">
		<div class="media-left" style="color: blue;">
			<h4>USER:</h4>
		</div>
		<div class="media-body">
			<h4 class="media-heading">Name: <%= user.name %></h4>
			Registered: <%= user.created_at %>
		</div>
	</li>
  <% end %>
</ul>
