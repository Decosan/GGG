<h1>Edit Page</h1>
<%= form_with(model: @post,local: true)do |f| %>
<div class='post_group'>
  <%= f.label :title, 'title' %>
  <%= f.text_field :title, class:'form-control' %>
</div>

<div class='post_group'>
  <%= f.label :content, 'content' %>
  <%= f.text_area :content, class:'form-control',row:2 %>
</div>
<%= f.submit 'Edit Post',class:'btn btn-primary btn-lg' %>
<% end %>
